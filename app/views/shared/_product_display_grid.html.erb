<div class="container">
            <div class="row">
              <% @skus.each do |sku| %>
              <div class="col-lg-4">
                <%= image_tag(sku.stock_photo_uri, class: 'list_image', size: '150x200') %><br/>
                <%= sku.name %> <br/>
                <%= "Price: $#{sku.price}" %> <br/>
              </div>
              <% end %>
            </div>
            <hr/>
</div>
